PREFIX rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:  <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd:   <http://www.w3.org/2001/XMLSchema#>
PREFIX owl:   <http://www.w3.org/2002/07/owl#>
PREFIX swrl:  <http://www.w3.org/2003/11/swrl#>
PREFIX swrlb: <http://www.w3.org/2003/11/swrlb#>
PREFIX vitro: <http://vitro.mannlib.cornell.edu/ns/vitro/0.7#>
PREFIX agu-index: <http://deepcarbon.net/ontology/agu-index#>
PREFIX bibo: <http://purl.org/ontology/bibo/>
PREFIX c4o: <http://purl.org/spar/c4o/>
PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dco: <http://deepcarbon.net/ontology/schema#>
PREFIX dcelem: <http://purl.org/dc/elements/1.1/>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX event: <http://purl.org/NET/c4dm/event.owl#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX fabio: <http://purl.org/spar/fabio/>
PREFIX geo: <http://aims.fao.org/aos/geopolitical.owl#>
PREFIX pvs: <http://vivoweb.org/ontology/provenance-support#>
PREFIX ero: <http://purl.obolibrary.org/obo/>
PREFIX scires: <http://vivoweb.org/ontology/scientific-research#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX vitro-public: <http://vitro.mannlib.cornell.edu/ns/vitro/public#>
PREFIX vivo: <http://vivoweb.org/ontology/core#>

#
# This example query gets 20 geographic locations
# and (if available) their labels
#
SELECT ?Project ?Description
WHERE
{
      <http://info.deepcarbon.net/individual/dfe2d75d3-c6a0-4c54-8ea5-ac2c0dbe08e6> <http://deepcarbon.net/ontology/schema#hasProject> ?o .
      ?o <http://www.w3.org/2000/01/rdf-schema#label> ?project .
      ?o <http://vivoweb.org/ontology/core#description> ?description .
      BIND(str(?project) AS ?Project)
      BIND(str(?description) AS ?Description)
}
LIMIT 20
