PREFIX bibo: <http://purl.org/ontology/bibo/>
PREFIX c4o: <http://purl.org/spar/c4o/>
PREFIX dcelem: <http://purl.org/dc/elements/1.1/>
PREFIX dco: <http://deepcarbon.net/ontology/schema#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX ero: <http://purl.obolibrary.org/obo/>
PREFIX event: <http://purl.org/NET/c4dm/event.owl#>
PREFIX fabio: <http://purl.org/spar/fabio/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX geo: <http://aims.fao.org/aos/geopolitical.owl#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX p.1: <http://purl.org/vocab/vann/>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX pvs: <http://vivoweb.org/ontology/provenance-support#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX scires: <http://vivoweb.org/ontology/scientific-research#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX swrl: <http://www.w3.org/2003/11/swrl#>
PREFIX swrlb: <http://www.w3.org/2003/11/swrlb#>
PREFIX vitro: <http://vitro.mannlib.cornell.edu/ns/vitro/0.7#>
PREFIX vitro-public: <http://vitro.mannlib.cornell.edu/ns/vitro/public#>
PREFIX vivo: <http://vivoweb.org/ontology/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
 
SELECT DISTINCT 
        ?Fieldwork_Title 
        ?Location
        ?Leader 
        ?Funding_Organization 
        ?DCO_ID
WHERE {
        ?fieldwork a dco:Fieldwork ;
                rdfs:label ?title ;
                dco:dcoId ?dco_id ;
                dco:fieldworkLeader ?person ;
                dco:fundingAgency ?org ;
                vivo:geographicFocus ?geo .
        ?person rdfs:label ?leader ;
                vivo:personInPosition ?position .
        ?position vivo:positionInOrganization ?org1 .
        ?org1 rdfs:label "Deep Energy Community" .
        ?org rdfs:label ?funding_organization .
        ?geo rdfs:label ?location .
        BIND(str(?title) AS ?Fieldwork_Title)
        BIND(str(?location) AS ?Location)
        BIND(str(?leader) AS ?Leader)
        BIND(str(?funding_organization) AS ?Funding_Organization)
        BIND(str(?dco_id) AS ?DCO_ID)
}